	<version>2.5.2-SNAPSHOT</version>
	<packaging>pom</packaging>
	<name>soapUI - Project Builder</name>
	<url>http://www.soapui.org</url>
	<description>The main soapUI POM used to build different parts of
		soapUI platform.
		Different profiles define different parts of soapUI and settings that can be
		used across all soapUI Maven 2 projects.</description>

	<inceptionYear>2005</inceptionYear>

	<profiles>
		<profile>
			<id>soapui-core</id>
			<activation>
			</activation>
			<modules>
				<module>../xmlbeans</module>
				<module>../core</module>
			</modules>
		</profile>
		<profile>
			<id>soapui-xmlbeans</id>
			<activation>
			</activation>
			<modules>
				<module>../xmlbeans</module>
			</modules>
		</profile>
		<profile>
			<id>soapui-plugins</id>
			<activation>
			</activation>
			<modules>
				<module>../maven-soapui-plugin</module>
				<module>../soapui-idea-plugin</module>
				<module>../soapui-eclipse-plugin</module>
				<!--
					<module>../soapui-netbeans6-plugin</module>
				-->
			</modules>
		</profile>
		<profile>
			<id>soapui-maven-plugin</id>
			<activation>
			</activation>
			<modules>
				<module>../maven-soapui-plugin</module>
			</modules>
		</profile>
	</profiles>

